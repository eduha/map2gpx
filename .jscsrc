{
    "preset": "airbnb",
    "validateIndentation": 4,
    "maximumLineLength": 200,
    "requireTrailingComma": {
        "ignoreSingleLine": true
    },
    "requireBlocksOnNewline": 1,
    "requirePaddingNewLinesAfterBlocks": {
        "allExcept": ["inArrayExpressions", "inProperties", "singleLine"]
    }
}